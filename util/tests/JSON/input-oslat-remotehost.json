{
    "benchmarks": [
        {
            "name": "oslat",
            "ids": "1",
            "mv-params": {
                "global-options": [
                    {
                        "name": "common-params",
                        "params": [
                            { "arg": "duration", "vals": [ "10" ], "role": "client" },
                            { "arg": "rtprio", "vals": [ "1" ], "role": "client" }
                        ]
                    }
                ],
                "sets": [
                    {
                        "include": "common-params"
                    }
                ]
            }
        }
    ],
    "endpoints": [
        {
            "type": "remotehost",
            "controller-ip": "CONTROLLER_IP",
            "host": "CI_ENDPOINT_HOST",
            "user": "CI_ENDPOINT_USER",
            "userenv": "alma8",
            "unique-project": 1,
            "server": 1,
            "client": 1,
            "config": [
                {
                    "targets": "all",
                    "settings": {
                        "cpu-partitioning": 1
                    }
                }
            ]
        }
    ],
    "run-params": {
	"num-samples": "1",
	"test-order": "s"
    }
}
